{
  "name": "react-svg-path",
  "version": "0.0.0-development",
  "description": "Generate svg path commands.",
  "author": "joemaddalone",
  "license": "MIT",
  "type": "module",
  "homepage": "https://joemaddalone.github.io/react-svg-path-docs/",
  "repository": {
    "type": "git",
    "url": "https://github.com/joemaddalone/react-svg-path"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src/*.js"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run types && npm run build",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "types": "tsc",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=16.0",
    "react-dom": ">=16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@joemaddalone/path": "^2.0.0",
    "semantic-release": "^24.2.7"
  }
}
